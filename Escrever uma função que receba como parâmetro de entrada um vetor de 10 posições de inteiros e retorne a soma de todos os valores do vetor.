#include <stdio.h>
int soma_valores (int vetor[10])
{ int s=0, i;
for (i=0; i<10; i++)
s = s + vetor[i];
return s;
}
main()
{
int v[10], i, soma;
printf(“Leitura dos elementos do vetor\n\n”);
for(i=0;i<10;i++)
{
printf(“\nElemento[%d]; “,i);
scanf(“%d”, &v[i]);
}
soma=soma_valores(v);
printf(“\n\nSoma dos elementos do vetor = %d”, soma);
}
